<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.husnimubarack.miwok" >

    <application
        android:allowBackup="true"

        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.husnimubarack.miwok.NumbersActivity"
            android:label="@string/numbers_name" />
        <activity
            android:name="com.example.husnimubarack.miwok.FamilyActivity"
            android:label="@string/family_name" />
        <activity
            android:name="com.example.husnimubarack.miwok.ColorActivity"
            android:label="@string/colors_name" />
        <activity
            android:name="com.example.husnimubarack.miwok.PhraseActivity"
            android:label="@string/phrases_name" />
    </application>

</manifest>